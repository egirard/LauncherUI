{
  "name": "@ourway/firebase-template",
  "version": "0.0.1",
  "description": "A template for making sveltekit/firebase projects.",
  "author": "Alex Nicolaou",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/anicolao/firebase-template.git"
  },
  "keywords": [
    "firebase",
    "svelte"
  ],
  "license": "MIT",
  "bugs": {
